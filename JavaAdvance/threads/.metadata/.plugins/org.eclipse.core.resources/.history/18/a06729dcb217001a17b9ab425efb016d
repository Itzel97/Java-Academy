
public class Desborde {
	public static void main (String [] args) {
		
		
		try {
			int a [] = new int [10];
			//arrays has only 10 elements
			a[6] = 9;
			
		} catch (Exception e) {
			// TODO: handle exception
			System.out.println(e);
		}
		finally {
			System.out.println("se imprime siempre..");
		}
		
		System.out.println("test");
	}
}
