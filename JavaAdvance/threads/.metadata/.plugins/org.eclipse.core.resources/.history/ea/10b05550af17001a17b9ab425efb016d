import java.util.ArrayList;
import java.util.List;

public class ThreadsTest {

	public static void main (String [] args) {
		
		List <String> letras = new ArrayList<>();
		letras.add("H");
		letras.add("O");
		letras.add("L");
		letras.add("A");
		
		for (String letra: letras ) {
			System.out.println(letra);
		}
	}
}
